{
    "name": "sallycms/core",
    "description": "Core Framework for SallyCMS",
    "type": "sallycms-app",
    "keywords": [
        "cms",
        "php",
        "mysql",
        "framework",
        "sallycms"
    ],
    "authors": [
        {
            "name": "webvariants GbR",
            "homepage": "http://www.webvariants.de/"
        }
    ],
    "version": "0.9.x-dev",
    "homepage": "http://www.sallycms.de/",
    "license": "MIT",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "fabpot/yaml",
                "version": "1.0.6",
                "autoload": {
                    "psr-0": {
                        "sfYaml": "lib/"
                    }
                },
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/fabpot/yaml/zipball/8a266aadce"
                }
            }
        },
        {
            "type": "pear",
            "url": "http://pear.phpunit.de/"
        },
        {
            "type": "pear",
            "url": "http://pear.symfony-project.com/"
        }
    ],
    "autoload": {
        "psr-0": {
            "sly_": "lib/"
        },
        "files": [
            "lib/functions.php",
            "lib/compatibility.php"
        ]
    },
    "require": {
        "php": ">=5.2.1",
        "fabpot/yaml": "1.0.6",
        "leafo/lessphp": "0.3.*@stable",
        "webvariants/babelcache": "1.2.*",
        "sallycms/composer-installer": "*",
        "xrstf/composer-php52": "~1.0"
    },
    "require-dev": {
        "pear-phpunit/phpunit": ">=3.6.10,<3.8",
        "pear-phpunit/dbunit": ">=1.1,<1.2",
        "doctrine/dbal": "~2.3.2"
    },
    "extra": {
        "branch-alias": {
            "dev-default": "0.9.x-dev"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "xrstf\\Composer52\\Generator::onPostInstallCmd"
        ],
        "post-update-cmd": [
            "xrstf\\Composer52\\Generator::onPostInstallCmd"
        ]
    }
}
