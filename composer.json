{
    "name": "sallycms/core",
    "description": "Core Framework for SallyCMS",
    "type": "sallycms-app",
    "keywords": [
        "cms",
        "php",
        "mysql",
        "framework",
        "sallycms"
    ],
    "authors": [
        {
            "name": "webvariants GbR",
            "homepage": "http://www.webvariants.de/"
        }
    ],
    "version": "0.9.x-dev",
    "homepage": "http://www.sallycms.de/",
    "license": "MIT",
    "autoload": {
        "psr-0": {
            "sly_": "lib/"
        },
        "files": [
            "lib/functions.php"
        ]
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/yaml": "~2.2",
        "leafo/lessphp": "~0.3.8",
        "webvariants/babelcache": "~1.2.17",
        "sallycms/composer-installer": "~1.1",
        "pimple/pimple": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phpunit/dbunit": "1.2.*",
        "doctrine/dbal": "~2.3.2"
    },
    "extra": {
        "branch-alias": {
            "dev-default": "0.9.x-dev"
        }
    }
}
