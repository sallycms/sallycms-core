{
    "name": "sallycms/core",
    "type": "sallycms-app",
    "keywords": [
        "cms",
        "php",
        "mysql",
        "framework",
        "sallycms"
    ],
    "version": "0.9.x-dev",
    "license": "MIT",
    "autoload": {
        "psr-0": {
            "sly_": "lib/"
        },
        "files": [
            "lib/functions.php",
            "lib/compatibility.php"
        ]
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/yaml": "~2.2",
        "leafo/lessphp": "0.3.*@stable",
        "webvariants/babelcache": "1.2.*",
        "sallycms/composer-installer": "~1.1",
        "pimple/pimple": "~1.0"
    },
    "require-dev": {
    },
    "extra": {
        "branch-alias": {
            "dev-default": "0.9.x-dev"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "xrstf\\Composer52\\Generator::onPostInstallCmd"
        ],
        "post-update-cmd": [
            "xrstf\\Composer52\\Generator::onPostInstallCmd"
        ]
    }
}
