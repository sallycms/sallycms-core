{
    "name": "sallycms/core",
    "type": "sallycms-app",
    "version": "0.9.x-dev",
    "license": "MIT",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "fabpot/yaml",
                "version": "1.0.6",
                "autoload": {
                    "psr-0": {
                        "sfYaml": "lib/"
                    }
                },
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/fabpot/yaml/zipball/8a266aadce"
                }
            }
        }
    ],
    "autoload": {
        "psr-0": {
            "sly_": "lib/"
        },
        "files": [
            "lib/functions.php",
            "lib/compatibility.php"
        ]
    },
    "require": {
        "php": ">=5.2.1",
        "fabpot/yaml": "1.0.6",
        "leafo/lessphp": "0.3.*@stable",
        "webvariants/babelcache": "1.2.*",
        "sallycms/composer-installer": "~1.1",
        "xrstf/composer-php52": ">=1.0.3,<2.0"
    },
    "require-dev": {
    },
    "extra": {
        "branch-alias": {
            "dev-default": "0.9.x-dev"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "xrstf\\Composer52\\Generator::onPostInstallCmd"
        ],
        "post-update-cmd": [
            "xrstf\\Composer52\\Generator::onPostInstallCmd"
        ]
    }
}
